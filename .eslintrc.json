{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "react-app",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "semi": ["error", "always"]
  }
}
